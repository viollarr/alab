<params>
	    <param name="template_layout" type="list" default="left_main_right" label="Template Layout" description="Template Layout">
			<option value="left_main_right">Left Main Right</option>
			<option value="left_right_main">Left Right Main</option>
			<option value="main_left_right">Main Left Right</option>
		</param>
		<param name="template_width" type="text" default="950px" label="Template Width" description="Set your Template Width in pixel or precent, basic width is 980px" />
		<param name="module_width" type="text" default="200px" label="Left+Right Module Width" description="Set your Module Width in pixel only, basic width is 200px" />
		<param name="cssThree" type="list" default="1" label="Enable CSS3 effects?" description="Enables nice CSS3 effects like rounded corner, text shadows etc. Only users with modern browsers can see the effects, Users with older and IE browsers will see the normal version. Turn off if you want to have the same user experience for all visitors">
			<option value="0">Off</option>
			<option value="1">On</option>
		</param>
	<param name="sliderHeight" type="text" default="300" size="5" label="Height of the Slider Area" description="Enter the height of the slider Area in px" />
		<param name="slider_width" type="text" default="910" label="Slider Width" description="Enter the width of the slider Area in px" />
		
		<param name="bounce" type="radio" default="slide" label="Bounce Slide Effect" description="Choose the type of slide effect">
			<option value="bounce">Bounce</option>
			<option value="slide">Slide</option>
		
		</param>
		<param name="sliderAuto" type="radio" default="1" label="Autoplay" description="Enables the autoplay function for the slider">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		
		<param name="sliderArrows" type="radio" default="1" label="Show Next/Prev. arrows" description="Show arrows on the left and right side to navigate forward and backwards">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="sliderTime" type="text" default="6000" size="5" label="Duration of the Slides change" description="Enter the time between one slide to another in ms" />
		<param name="slimbox" type="radio" default="1" label="Activate Slimbox" description="Loading Slimbox JS">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="showSuckerfish" type="radio" default="1" label="Activate MooMenu" description="enables Moomenu via JS">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>
</install>
